<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.morichan.retuss.window.DeleteMessageDialogController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label text="削除するメッセージを選択してください。" />
            <ListView fx:id="messageList" prefHeight="200.0" prefWidth="200.0" />
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonHandler" text="メッセージを削除" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</Pane>
